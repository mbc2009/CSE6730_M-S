@article{xiao_Slippery_2019,
  title = {Slippery for Scaling Resistance in Membrane Distillation: A Novel Porous Micropillared Superhydrophobic Surface},
  shorttitle = {Slippery for Scaling Resistance in Membrane Distillation},
  author = {Xiao, Zechun and Zheng, Rui and Liu, Yongjie and He, Hailong and Yuan, Xiaofei and Ji, Yunhui and Li, Dongdong and Yin, Huabing and Zhang, Yuebiao and Li, Xuemei and He, Tao},
