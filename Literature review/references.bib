@article{gunnarsson_Impact_2005,
  title = {Impact of Silica Scaling on the Efficiency of Heat Extraction from High-Temperature Geothermal Fluids},
  author = {Gunnarsson, Ingvi and Arn{\'o}rsson, Stef{\'a}n},
  year = {2005},
