@article{cahill_Nanoscale_2014,
  title = {Nanoscale Thermal Transport. {{II}}. 2003--2012},
  author = {Cahill, David G. and Braun, Paul V. and Chen, Gang and Clarke, David R. and Fan, Shanhui and Goodson, Kenneth E. and Keblinski, Pawel and King, William P. and Mahan, Gerald D. and Majumdar, Arun and Maris, Humphrey J. and Phillpot, Simon R. and Pop, Eric and Shi, Li},
