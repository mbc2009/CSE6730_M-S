@article{zhu_Interlayered_2023,
  title = {Interlayered Interface of a Thin Film Composite Janus Membrane for Sieving Volatile Substances in Membrane Distillation},
  author = {Zhu, Zhigao and Liu, Zhu and Tan, Guangming and Qi, Junwen and Zhou, Yujun and Li, Jiansheng},
  year = {2023},
  month = may,
  journal = {Environmental Science \& Technology},
