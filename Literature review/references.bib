@article{zhao_Nb2CTx_2022,
  title = {A {{Nb2CTx}}/Sodium Alginate-Based Composite Film with Neuron-like Network for Self-Powered Humidity Sensing},
  author = {Zhao, {\relax QN} and Jiang, {\relax YD} and Duan, {\relax ZH} and Yuan, Z and Zha, {\relax JJ} and Wu, {\relax ZK} and Huang, Q and Zhou, Z and Li, H and He, F and Su, {\relax YJ} and Tan, {\relax CL} and Tai, {\relax HL}},
  year = {2022},
