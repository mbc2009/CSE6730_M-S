@article{shi_Dynamic_2018,
  title = {Dynamic Contact Angle Hysteresis in Liquid Bridges},
  author = {Shi, Zhang and Zhang, Yi and Liu, Mingchao and Hanaor, Dorian A. H. and Gan, Yixiang},
