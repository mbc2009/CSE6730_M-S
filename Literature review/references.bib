@article{chen_Fabrication_2018,
  title = {Fabrication of Highly Transparent Slippery Surfaces with Omniphobicity by an Improved Process Using Non-Solvent-Induced Phase Separation},
  author = {Chen, {\relax CC} and Chen, {\relax CJ} and Chen, {\relax SA} and Li, {\relax WH} and Yang, {\relax YM}},
