  shorttitle = {Reversing Membrane Wetting in Membrane Distillation},
  author = {Warsinger, David M. and Servi, Amelia and Connors, Grace B. and Mavukkandy, Musthafa O. and Arafat, Hassan A. and Gleason, Karen K. and V, John H. Lienhard},
  year = {2017},
