@article{robinson_Nucleation_1970,
  title = {Nucleation Kinetics of Gold Deposition onto {{UHV}} Cleaved Surfaces of {{KCl}} and {{NaF}}},
  author = {Robinson, V.N.E. and Robins, J.L.},
  year = {1970},
  month = may,
  journal = {Thin Solid Films},
  volume = {5},
  number = {5},
  pages = {313--327},
  issn = {0040-6090},
  doi = {10.1016/0040-6090(70)90104-5},
  url = {https://www.sciencedirect.com/science/article/pii/0040609070901045},
  abstract = {An experimental investigation is described in which the nucleation kinetics of gold deposited from the vapour phase onto UHV cleaved surfaces of KCl and NaF have been studied within the temperature range of 60--370 {$^\circ$}C and for atom arrival rates of 1{\texttimes}1012-3{\texttimes}1013 atoms cm-2 sec-1. In a given deposit, the density of nuclei increased with time to a well defined saturation value, the approach to this value being monotonic and of approximately exponential form. In general, the nucleation rate at constant temperature was observed to be proportional to the square of the atom arrival rate, indicating that a pair of adsorbed atoms constitutes a stable nucleus. The saturation density of nuclei was observed to increase as the atom arrival rate increased or as the temperature decreased. It is shown that existing theories, which have not previously been fully tested, are inadequate to explain the features observed. The source of this inadequacy is discussed and the basis of a more successful theory is proposed.}
}

