  shorttitle = {Computational Fluid Dynamics Simulation Study of Hypersaline Water Desalination via Membrane Distillation},
  author = {Afsari, Morteza and Ghorbani, Amir Hossein and Asghari, Morteza and Shon, Ho Kyong and Tijing, Leonard D.},
