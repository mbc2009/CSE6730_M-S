  keywords = {cite only},
  file = {/Users/liuhonglin/Dropbox (GaTech)/Application/Zotero/ZotFile/attachment/Xyla et al. - 1992 - The inhibition of calcium carbonate precipitation .pdf;/Users/liuhonglin/Dropbox (GaTech)/Application/Zotero/storage/YQIRZCJM/002197979290344L.html}
