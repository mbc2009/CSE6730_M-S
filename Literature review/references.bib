  author = {Deng, Li and Ye, Haohui and Li, Xiong and Li, Peiyun and Zhang, Jiawei and Wang, Xuefen and Zhu, Meifang and Hsiao, Benjamin S.},
  year = {2018},
  month = nov,
  journal = {Separation and Purification Technology},
  volume = {206},
  pages = {14--25},
  issn = {1383-5866},
  doi = {10.1016/j.seppur.2018.05.035},
