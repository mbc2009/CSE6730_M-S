@article{li_Efficient_2022,
  title = {Efficient Synergism of {{K}}{\textsubscript{2}}{{FeO}}{\textsubscript{4}} Preoxidation/ {{MIEX}} Adsorption in Ultrafiltration Membrane Fouling Control and Mechanisms},
