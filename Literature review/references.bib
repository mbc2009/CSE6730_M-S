  isbn = {978-0-19-956428-6},
  langid = {english},
  file = {/Users/liuhonglin/Dropbox (GaTech)/Application/Zotero/storage/CJI7CGKZ/Atkins and Paula - 2011 - Physical Chemistry for the Life Sciences.pdf}
}

