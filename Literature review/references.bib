@article{jia_Asymmetric_2022,
  title = {Asymmetric Superwetting Janus Structure for Fouling- and Scaling-Resistant Membrane Distillation},
  author = {Jia, Yuandong and Guan, Kecheng and Zhang, Pengfei and Shen, Qin and Li, Zhan and Istirokhatun, Titik and Matsuyama, Hideto},
  year = {2022},
  month = sep,
  journal = {Journal of Membrane Science},
  volume = {657},
