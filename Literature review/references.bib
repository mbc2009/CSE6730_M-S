  author = {Chen, {\relax YM} and Yang, Y and Xiong, Y and Zhang, L and Xu, {\relax WH} and Duan, {\relax GG} and Mei, {\relax CT} and Jiang, {\relax SH} and Rui, {\relax ZH} and Zhang, K},
