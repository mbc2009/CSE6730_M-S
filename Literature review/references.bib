@article{al-anezi_Effect_2006,
  title = {Effect of Carbon Dioxide in Seawater on Desalination: A Comprehensive Review},
  shorttitle = {Effect of Carbon Dioxide in Seawater on Desalination},
  author = {Al-Anezi, Khalid and Hilal, Nidal},
  year = {2006},
