}

@article{shih_Morphometric_2005,
  title = {Morphometric Characterization of Calcium Sulfate Dihydrate (Gypsum) Scale on Reverse Osmosis Membranes},
  author = {Shih, Wen-Yi and Rahardianto, Anditya and Lee, Ron-Wai and Cohen, Yoram},
  year = {2005},
  month = apr,
  journal = {Journal of Membrane Science},
  volume = {252},
