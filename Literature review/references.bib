@article{wilson_Inhibition_2013,
  title = {Inhibition of Ice Nucleation by Slippery Liquid-Infused Porous Surfaces ({{SLIPS}})},
  author = {Wilson, P.W. and Lu, W. and Xu, H. and Kim, P. and Kreder, M.J. and Alvarenga, J. and Aizenberg, J.},
