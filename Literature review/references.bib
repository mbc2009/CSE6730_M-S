  author = {{Guillen-Burrieza}, Elena and Thomas, Rinku and Mansoor, Bilal and Johnson, Daniel and Hilal, Nidal and Arafat, Hassan},
  year = {2013},
  month = jul,
  journal = {Journal of Membrane Science},
  volume = {438},
