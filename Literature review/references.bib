  shorttitle = {Wetting Phenomena in Membrane Distillation},
  author = {Rezaei, Mohammad and Warsinger, David M. and Lienhard V, John H. and Duke, Mikel C. and Matsuura, Takeshi and Samhaber, Wolfgang M.},
  year = {2018},
