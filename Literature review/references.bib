@article{sun_Multiscale_2020,
  title = {Multiscale Connectivity Characterization of Marine Shales in Southern {{China}} by Fluid Intrusion, Small-Angle Neutron Scattering ({{SANS}}), and {{FIB-SEM}}},
  author = {Sun, {\relax MD} and Zhang, {\relax LH} and Hu, {\relax QH} and Pan, {\relax ZJ} and Yu, {\relax BS} and Sun, {\relax LF} and Bai, {\relax LF} and Fu, {\relax HJ} and Zhang, {\relax YF} and Zhang, C and Cheng, G},
