@article{zheng_Insight_2020,
  title = {Insight into the Magnetic Lime Coagulation-Membrane Distillation Process for Desulfurization Wastewater Treatment: From Pollutant Removal Feature to Membrane Fouling},
  author = {Zheng, {\relax LB} and Jiao, {\relax YY} and Zhong, H and Zhang, C and Wang, J and Wei, {\relax YS}},
