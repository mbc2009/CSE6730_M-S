@article{owens_Oreology_2022,
  title = {On Oreology, the Fracture and Flow of ``Milk's Favorite Cookie {\textsuperscript{{\textregistered}}} ''},
  author = {Owens, Crystal E. and Fan, Max R. and Hart, A. John and McKinley, Gareth H.},
  year = {2022},
