@article{zhao_Highly_2018,
  title = {Highly {{Stable Graphene-Based Nanocomposite}} ({{GO}}--{{PEI}}--{{Ag}}) with {{Broad-Spectrum}}, {{Long-Term Antimicrobial Activity}} and {{Antibiofilm Effects}}},
