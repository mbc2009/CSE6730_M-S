@article{raza_Molecular_2022,
  title = {Molecular Dynamics Simulation of Steady-State Droplet Condensation on a Fiber in Direct Contact Membrane Distillation Settings},
  author = {Raza, Saqlain and He, Jixiong and Tafreshi, Hooman V. and Liu, Jun},
  year = {2022},
  month = dec,
  journal = {Journal of Molecular Liquids},
